def merge(left, right):
    result = []
    i, j = 0, 0
    while i < len(left) and j< len(right):
        if left[i] < right[j]:
            result.apend(left[i])
            i += 1
        else:
            result.append(right[j])
            j += 1
    while (i < len(left)):
        result.append(left[i])
    while (j < len(right)):
        result.append(right[j])
        j += 1
    return result
